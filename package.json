{
	"name": "jsmeow",
	"version": "1.0.6",
	"description": "NodeJS for external Game Hacking",
	"types": "dist/index.d.ts",
	"main": "./dist/index.js",
	"author": "DroidXrX <droidxrx@gmail.com> (https://github.com/droidxrx)",
	"homepage": "https://github.com/droidxrx/JSMeow#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/droidxrx/JSMeow.git"
	},
	"license": "MIT",
	"keywords": [
		"JSMeow",
		"pyMeow",
		"hacking",
		"esp",
		"read-memory",
		"write-memory"
	],
	"private": false,
	"engines": {
		"node": ">=16.14.0"
	},
	"os": [
		"win32"
	],
	"cpu": [
		"x64"
	],
	"scripts": {
		"build-addon": "rimraf CMakeLists.txt && node build.js &&cmake-js rebuild --cxx -r node -v 16.14.0 -a x64 --config Release && rimraf CMakeLists.txt",
		"build-ts": "rollup -c rollup.config.js -w",
		"build-dts": "rimraf dist/*.d.ts && dts-bundle-generator -o dist/index.d.ts --project ./tsconfig.json src/ts/index.ts --no-banner",
		"build-all": "npm run build-addon && npm run build-dts && npm run build-ts",
		"all-overlay": "ts-node example/all-overlay.ts -T -P ./tsconfig.json",
		"test-overlay": "ts-node example/test-overlay.ts -T -P ./tsconfig.json",
		"test-assault-cube": "ts-node example/assault-cube.ts -T -P ./tsconfig.json",
		"test-cs-go": "ts-node example/cs_go.ts -T -P ./tsconfig.json"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@types/node": "^17.0.21",
		"@types/superagent": "^4.1.15",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.11.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.25.4",
		"fs-extra": "^10.0.1",
		"node-addon-api": "^4.3.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.70.1",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-ts": "^2.0.5",
		"superagent": "^7.1.1",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2"
	},
	"clean-publish": {
		"files": [
			"build",
			"example",
			"external",
			"src",
			".clang-format",
			".gitignore",
			"build.js",
			"CMakeLists.txt",
			"rollup.config.js",
			"tsconfig.json"
		],
		"withoutPublish": true,
		"tempDir": "package"
	}
}
